package kriteria;

/**
 *
 * @author donia
 */
public class Kriteria {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
    int umur = 24;
    String nama = "Doni Ari Pradana";
    boolean cantik = true;
    if (umur>23 && cantik==true){
        System.out.println("Saya bersedia");
    }
    else if (umur>23){
        System.out.println("Sebentar, saya pikir-pikir dulu");
    }
    else {
        System.out.println("Maaf, saya tidak bersedia");
    }
    }
    
}
